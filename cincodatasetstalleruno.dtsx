                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select US.UsuarioId, GEO.Latitud, GEO.Longitud, DEM.TamañoFamiliar, DEM.Edad, GEN.NombreGenero, DEM.Ingresos, EST.NivelEstrato, EV.NombreEvento from [dbo].[Usuarios] as US
INNER JOIN [dbo].[UsuariosXGeograficos] as USGEO
ON US.UsuarioId = USGEO.UsuarioId
INNER JOIN [dbo].[Geograficos] as GEO
ON USGEO.UbicacionId = GEO.UbicacionId
INNER JOIN [dbo].[Demograficos] as DEM
ON US.DemograficoId=DEM.DemograficoId
INNER JOIN [dbo].[Generos] as GEN
ON DEM.GeneroId = GEN.GeneroId
INNER JOIN [dbo].[Estratos] AS EST
ON DEM.EstratoId = EST.EstratoId
INNER JOIN [dbo].[UsuariosXEventos] as USEVENTO
ON US.UsuarioId = USEVENTO.UsuarioId
INNER JOIN [dbo].[Eventos] AS EV
ON USEVENTO.EventoId = EV.EventosId</property>
