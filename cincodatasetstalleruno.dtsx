                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select US.UsuarioId, GEO.Latitud, GEO.Longitud, DEM.TamañoFamiliar, DEM.Edad, GEN.NombreGenero, DEM.Ingresos, EST.NivelEstrato, EV.NombreEvento from [dbo].[Usuarios] as US
